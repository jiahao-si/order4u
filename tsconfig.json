{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "outDir": "./tsbuild",
    "sourceMap": true,
    "jsx": "react",
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "experimentalDecorators": true,
    "suppressExcessPropertyErrors": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}